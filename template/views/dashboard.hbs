            </div>
        </div>
    </div>

    {{>footer}}
</body>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.17/dist/sweetalert2.all.min.js"></script>
<script>
    const aavtar = document.getElementById("aavtar");
    const editbtn = document.getElementById("change");
    const avatarbox = document.querySelector('.avatar_box');
    const allavatr = document.querySelectorAll(".row_wrap img");
    editbtn.addEventListener("click", () => {
        if (avatarbox.style.display === "grid") {
            avatarbox.style.display = "none";
        } else {
            avatarbox.style.display = "grid";
        }
    });

    allavatr.forEach((aav, i) => {
        aav.addEventListener("click", (e) => {
            aavtar.src = aav.src;
        })
    })
    function logout() {
        Swal.fire({
            title: "Are you sure?",
            text: "You will be logged out!",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, log me out!",
        }).then((result) => {
            if (result.isConfirmed) {
                // User confirmed logout, redirect to logout route
                window.location.href = "/logout";
                console.log("user  logout")

            } else {
                console.log("user cancel the request of logout")
            }
        });
    }
</script>

</html>